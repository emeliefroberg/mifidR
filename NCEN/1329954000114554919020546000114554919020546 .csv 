"8927","<TYPE>INTERNAL CONTROL RPT"
"8928","<SEQUENCE>3"
"8929","<FILENAME>jhfiii.htm"
"8930","<TEXT>"
"8931","<HTML><HEAD>"
"8932","<TITLE>HTML</TITLE>"
