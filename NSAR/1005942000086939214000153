"3566","PUTNAM DIVERSIFIED INCOME TRUST,"
"3567","PUTNAM EQUITY INCOME FUND,"
"3568","PUTNAM EUROPE EQUITY FUND,"
"3569","THE PUTNAM FUND FOR GROWTH AND INCOME,"
"3570","PUTNAM FUNDS TRUST,"
"3571","GEORGE PUTNAM BALANCED FUND,"
"3572","      PUTNAM GLOBAL EQUITY FUND,"
