"698","<DESCRIPTION>ACCOUNTANT'S REPORT ON INTERNAL CONTROL"
"699","<TEXT>"
"700","<html>"
"701","<head>"
"702","    <title>ex77b.htm</title>"
"703","    <!-- Licensed to: edgarfilepoint -->"
"704","    <!-- Document Created using EDGARizerAgent 5.2.3.0 -->"
