"10715","	<FONT STYLE=font-family: Times New Roman, Times, Serif; font-size: 7pt>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"
"10716","        </FONT><FONT STYLE=font-family: Garamond, Times, Serif>Office of the Chief Compliance Officer</FONT></P>"
"10717","        <P STYLE=font: 11pt Calibri, Helvetica, Sans-Serif; margin: 6pt 34.25pt 0 23.1pt; text-indent: -18.05pt>"
"10718","	<FONT STYLE=font-family: Arial, Helvetica, Sans-Serif>&#8226;</FONT>"
"10719","	<FONT STYLE=font-family: Times New Roman, Times, Serif; font-size: 7pt>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"
"10720","        </FONT><FONT STYLE=font-family: Garamond, Times, Serif>Fund Administration</FONT></P>"
"10721","        <P STYLE=font: 11pt Calibri, Helvetica, Sans-Serif; margin: 6pt 12.75pt 0 23.1pt; text-indent: -18.05pt>"
"10722","	<FONT STYLE=font-family: Arial, Helvetica, Sans-Serif>&#8226;</FONT>"
"10723","	<FONT STYLE=font-family: Times New Roman, Times, Serif; font-size: 7pt>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"
"10724","        </FONT><FONT STYLE=font-family: Garamond, Times, Serif>Investment Management Services,</FONT></P>"
"10725","        <P STYLE=font: 11pt Calibri, Helvetica, Sans-Serif; margin: 6pt 34.25pt 0 23.1pt; text-indent: -18.05pt>"
"10726","	<FONT STYLE=font-family: Arial, Helvetica, Sans-Serif>&#8226;</FONT>"
