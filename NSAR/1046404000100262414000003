"692","<p style=margin:0in;margin-bottom:.0001pt;><font face=courier new lang=EN-US style=font-size:10.5pt;>T. Rowe Price"
"693","Real Estate Fund</font></p>"
"694","<p style=margin:0in;margin-bottom:.0001pt;><font face=courier new lang=EN-US style=font-size:10.5pt;>&nbsp;</font></p>"
