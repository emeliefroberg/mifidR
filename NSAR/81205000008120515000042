"1752","<SEQUENCE>6"
"1753","<FILENAME>q77q1_subadvisory.htm"
"1754","<DESCRIPTION>SUB ADVISORY"
"1755","<TEXT>"
"1756","<!doctype html PUBLIC -//IETF//DTD HTML//EN>"
"1757","<HTML>"
