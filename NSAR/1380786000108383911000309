"781","SIGNATURE   STEPHEN D. FOY"
"782","TITLE       V.P.& CONTROLLER"
"783","</TEXT>"
"784","</DOCUMENT>"
"785","<DOCUMENT>"
"786","<TYPE>EX-99.77B ACCT LTTR"
"787","<SEQUENCE>2"
