"2904","<P style=MARGIN: 0in 0in 0pt 0.5in; TEXT-INDENT: -0.5in; TEXT-ALIGN: justify><FONT lang=EN-US style=FONT-SIZE: 11pt face=times new roman></FONT>&nbsp;</P>"
"2905","<P style=MARGIN: 0in 0in 0pt 0.5in; TEXT-INDENT: -0.5in; TEXT-ALIGN: justify><FONT lang=EN-US style=FONT-SIZE: 11pt face=times new roman>(d)&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &#8220;Authorized Person&#8221; shall mean (i) any officer of the Fund; (ii) or any person, whether or not such person is an officer or employee of the Fund, duly authorized by an authorized officer of the Fund to give Oral Instructions or Written Instructions on behalf of the Fund as indicated in writing to JHSS Agent from time to time. </FONT></P>"
"2906","<P style=MARGIN: 0in 0in 0pt 0.5in; TEXT-INDENT: -0.5in; TEXT-ALIGN: justify><FONT lang=EN-US style=FONT-SIZE: 11pt face=times new roman></FONT>&nbsp;</P>"
