"5704","<P ALIGN=LEFT>President and Treasurer"
"5705","</P>"
"5706","<P ALIGN=LEFT><B>STRATEGIC ADVISERS, INC.</B>"
"5707","</P>"
"5708","<P ALIGN=LEFT>By:  <U>/s/ J. Gregory Wass</U>"
"5709","</P>"
"5710","<P ALIGN=LEFT>J. Gregory Wass"
"5711","</P>"
"5712","<P ALIGN=LEFT>Assistant Treasurer"
"5713","</P>"
"5714","<P ALIGN=LEFT>"
"5715","</P>"
