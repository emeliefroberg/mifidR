"7239","<P ALIGN=CENTER><B><U>Name of Trust</U></B>"
"7240","</P>"
"7241","<P ALIGN=CENTER>&nbsp;"
"7242","</P>"
"7243","</TD>"
"7244","<TD ALIGN=CENTER VALIGN=TOP>"
"7245","<P ALIGN=CENTER><B><U>Name of Portfolio</U></B>"
"7246","</P>"
"7247","</TD>"
"7248","<TD ALIGN=CENTER VALIGN=TOP>"
"7249","<P ALIGN=CENTER><B><U>Effective Date</U></B>"
"7250","</P>"
"7251","</TD>"
"7252","</TR>"
"7253","<TR>"
"7254","<TD ALIGN=LEFT VALIGN=TOP>"
"7255","<P ALIGN=LEFT>Fidelity Rutland Square Trust II"
"7256","</P>"
"7257","</TD>"
"7258","<TD ALIGN=LEFT VALIGN=TOP>"
"7259","<P ALIGN=LEFT>Strategic Advisers Core Income Fund"
"7260","</P>"
"7261","</TD>"
"7262","<TD ALIGN=CENTER VALIGN=TOP>"
"7263","<P ALIGN=CENTER>03/05/10"
"7264","</P>"
"7265","</TD>"
