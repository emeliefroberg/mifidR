"9544","<TR><TD style=margin-top:0px valign=top width=319.2><P style=margin:0px; font-size:11pt>0.525%</P>"
"9545","</TD><TD style=margin-top:0px valign=top width=319.2><P style=margin:0px; font-size:11pt align=center>Of any additional average daily net assets</P>"
"9546","</TD></TR>"
"9547","</TABLE>"
"9548","<DIV style=width:624px><P style=margin:0px; font-size:11pt><FONT style=background-color:#FFFF00>&nbsp;</FONT></P>"
"9549","<P style=margin-top:0px; margin-bottom:-2px; width:48px; font-size:11pt; float:left>2.</P>"
"9550","<P style=margin:0px; text-indent:-2px; font-size:11pt align=justify>This Third Amendment shall be effective as of the date hereof, and shall continue until terminated in accordance with Section&#160;14 of the Agreement. </P>"
"9551","<P style=margin:0px; font-size:11pt; clear:left align=justify>&#160;</P>"
"9552","<P style=margin-top:0px; margin-bottom:-2px; width:48px; font-size:11pt; float:left>3.</P>"
"9553","<P style=margin:0px; text-indent:-2px; font-size:11pt align=justify>All terms of the Agreement shall remain in force to the extent not inconsistent with the above. </P>"
"9554","<P style=margin:0px; font-size:11pt; clear:left align=justify>&#160;</P>"
