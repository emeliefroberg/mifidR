"3495","    <TD vAlign=top></TD>"
"3496","    <TD vAlign=top width=100%><FONT face=Times New Roman size=2>litigation"
"3497","      expenses or other legal expenses relating to a specific"
"3498","class;</FONT></TD></TR>"
"3499","  <TR>"
"3500","    <TD vAlign=top></TD>"
"3501","    <TD vAlign=top colSpan=3>&nbsp;</TD></TR>"
"3502","  <TR>"
"3503","    <TD vAlign=top noWrap></TD>"
