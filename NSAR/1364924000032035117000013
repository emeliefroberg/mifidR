"7801","<P style=margin:0px; font-size:11pt><B>FIDELITY RUTLAND SQUARE TRUST II</B></P>"
"7802","<P style=margin:0px><BR></P>"
"7803","<P style=margin:0px><BR></P>"
"7804","<P style=margin-top:0px; margin-bottom:-2px; width:32px; font-size:11pt; float:left>By:</P>"
"7805","<P style=margin:0px; text-indent:-2px; font-size:11pt><U>/s/ Adrien Deberghes</U></P>"
