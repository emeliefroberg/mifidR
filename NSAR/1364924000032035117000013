"8447","<P style=margin:0px; font-size:11pt><B>FIDELITY RUTLAND SQUARE TRUST II</B></P>"
"8448","<P style=margin:0px><BR></P>"
"8449","<P style=margin:0px><BR></P>"
"8450","<P style=margin-top:0px; margin-bottom:-2px; width:32px; font-size:11pt; float:left>By:</P>"
"8451","<P style=margin:0px; text-indent:-2px; font-size:11pt><U>/s/ Adrien Deberghes</U></P>"
"8452","<P style=margin:0px; text-indent:32px; font-size:11pt; clear:left>Name: &nbsp;Adrien Deberghes</P>"
"8453","<P style=margin:0px; text-indent:32px; font-size:11pt>Title: &nbsp;&nbsp;&nbsp;&nbsp;Treasurer</P>"
