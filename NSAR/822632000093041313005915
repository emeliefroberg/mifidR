"787","<P STYLE=font: 10pt/10pt Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P>"
"788","<P STYLE=font: 11pt/104% Arial, Helvetica, Sans-Serif; margin: 1.5pt 3.05pt 0 1in; text-align: justify>&nbsp;</P>"
"789","<table cellspacing=0 cellpadding=0 style=width: 100%; border-collapse: collapse>"
"790","<tr style=vertical-align: top>"
"791","    <td style=width: 63%; padding-left: 4.5pt; font: 10pt/115% Calibri, Helvetica, Sans-Serif><img src=image_002.jpg alt= style=height: 138px; width: 457px></td>"
"792","    <td style=width: 13%; font: 10pt/115% Calibri, Helvetica, Sans-Serif>&nbsp;</td>"
"793","    <td style=width: 24%>"
"794","        <P STYLE=font: 10pt/normal Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P>"
"795","        <P STYLE=font: 10pt/normal Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P>"
"796","        <P STYLE=font: 10pt/normal Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P>"
"797","        <P STYLE=font: 10pt/normal Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P>"
"798","        <P STYLE=font: 10pt/8.6pt Arial, Helvetica, Sans-Serif; margin: 0>&nbsp;</P></td></tr>"
